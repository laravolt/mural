{
    "name": "laravolt/mural",
    "description": "Laravel comment stream",
    "keywords": [
        "laravel",
        "mural",
        "comment",
    ],
    "homepage": "https://github.com/laravolt/mural",
    "license": "MIT",
    "authors": [
        {
            "name": "Bayu Hendra Winata",
            "email": "uyab.exe@gmail.com",
            "homepage": "http://id-laravel.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Laravolt\\Mural\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravolt\\Mural\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
